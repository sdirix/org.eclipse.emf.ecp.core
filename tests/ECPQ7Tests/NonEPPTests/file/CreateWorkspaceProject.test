Content-Type: text/ecl
Entry-Name: .content

with [get-view "Model Explorer" | get-tree] {
    get-property itemCount | equals 0 | verify-true
}
get-view "Model Explorer" | get-tree | get-menu "Create new project" | click
with [get-window "Create Project"] {
    get-combo -after [get-label "Provider:"] | select "Eclipse Workspace (Experimental)"
    get-editbox -after [get-label "Project Name:"] | set-text TestProject
}
with [get-window "Create Project"] {
    get-button "Create empty project" | get-property selected | equals true | verify-true
    get-button "Import existing XMI-File" | get-property selected | equals false | verify-true
    get-label "Filename:" | get-property caption | equals "Filename:" | verify-true
    get-label "Root Class:" | get-property caption | equals "Root Class:" | verify-true
}
with [get-window "Create Project"] {
    get-button "Browse Workspace ..." | click
    with [get-window "New XMI File"] {
        get-button "Create New Project ..." | click
        with [get-window "New Project"] {
            get-editbox -after [get-label "Project name:"] | set-text TestProject
            get-button Finish | click
        }
        get-tree | select TestProject
        get-editbox -after [get-label "File name:"] | set-text "testProject.xmi"
        get-button Finish | click
    }
}
get-window "Create Project" | get-editbox -after [get-label "Filename:"] | get-property text 
    | equals "platform:/resource/TestProject/testProject.xmi" | verify-true
get-window "Create Project" | get-button Finish | get-property enablement | equals false | verify-true
with [get-window "Create Project"] {
    get-button -after [get-label "Root Class:"] | click
    get-window "Choose Root Class" | try { get-tree | select "League" } -catch { get-tree | select "bowling/League" } | double-click
}
get-window "Create Project" | get-button Finish | get-property enablement | equals true | verify-true
get-window "Create Project" | get-button Finish | click
with [get-view "Model Explorer" | get-tree] {
    get-property itemCount | equals 1 | verify-true
    get-property "getItems().TreeItem[0].getText()" | equals TestProject | verify-true
}
with [get-view "Model Explorer" | get-tree] {
    get-item TestProject | get-property childCount | equals 1 | verify-true
    get-item TestProject | get-property "getItems().TreeItem[0].getText()" | equals League | verify-true
}
