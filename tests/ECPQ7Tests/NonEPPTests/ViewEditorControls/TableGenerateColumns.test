Content-Type: text/ecl
Entry-Name: .content

try {
get-editor View | get-button "Link Root EClass" | click
with [get-window "Select an EClass."] {
    get-tree | select "bowling/League \\[org.eclipse.emf.emfstore.bowling.League\\]"
    get-button OK | click
}
get-view "Model Explorer" | get-tree | select "Project/View" | get-menu TableControl | click
get-editor TableControl | get-button "Create and link new Domain Model Reference" | click
with [get-window "New Reference Element"] {
    get-button "Link Domain Model EFeature" | click
    with [get-window "Select a EStructuralFeature"] {
        get-tree | select "players : Player"
        get-button OK | click
    }
    get-button Finish | click
}
get-view "Model Explorer" | get-tree | select "Project/View/TableControl"
get-view "Model Explorer" | get-tree | get-menu "Generate Columns" | get-property caption | equals "Generate Columns" 
    | verify-true
get-view "Model Explorer" | get-tree | select "Project/View/TableControl" | get-menu "Generate Columns" | click
with [get-window -class WizardDialog | get-table] {
    get-property itemCount | equals 9 | verify-true
    get-item "name : EString" | get-property caption | equals "name : EString" | verify-true
    get-item "name : EString" | get-property checked | equals false | verify-true
    get-item "height : EDouble" | get-property caption | equals "height : EDouble" | verify-true
    get-item "height : EDouble" | get-property checked | equals false | verify-true
    get-item "eMails : EString" | get-property caption | equals "eMails : EString" | verify-true
    get-item "eMails : EString" | get-property checked | equals false | verify-true
}
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | check
    get-item "height : EDouble" | check
}
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | get-property caption | equals "name : EString" | verify-true
    get-item "name : EString" | get-property checked | equals true | verify-true
    get-item "height : EDouble" | get-property caption | equals "height : EDouble" | verify-true
    get-item "height : EDouble" | get-property checked | equals true | verify-true
}
with [get-window -class WizardDialog] {
    get-editbox -after [get-label Search] | set-text eMails
    get-table | get-item "eMails : EString" | check
}
with [get-window -class WizardDialog | get-table] {
    get-item "eMails : EString" | get-property caption | equals "eMails : EString" | verify-true
    get-item "eMails : EString" | get-property checked | equals true | verify-true
}
get-window -class WizardDialog | get-editbox -after [get-label Search] | set-text ""
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | get-property caption | equals "name : EString" | verify-true
    get-item "name : EString" | get-property checked | equals true | verify-true
    get-item "height : EDouble" | get-property caption | equals "height : EDouble" | verify-true
    get-item "height : EDouble" | get-property checked | equals true | verify-true
    get-item "eMails : EString" | get-property caption | equals "eMails : EString" | verify-true
    get-item "eMails : EString" | get-property checked | equals true | verify-true
    get-item "dateOfBirth : EDate" | get-property caption | equals "dateOfBirth : EDate" | verify-true
    get-item "dateOfBirth : EDate" | get-property checked | equals false | verify-true
    get-item "isProfessional : EBoolean" | get-property caption | equals "isProfessional : EBoolean" | verify-true
    get-item "isProfessional : EBoolean" | get-property checked | equals false | verify-true
    get-item "numberOfVictories : EInt" | get-property caption | equals "numberOfVictories : EInt" | verify-true
    get-item "numberOfVictories : EInt" | get-property checked | equals false | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property caption 
        | equals "playedTournamentTypes : TournamentType" | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property checked | equals false | verify-true
    get-item "winLossRatio : EBigDecimal" | get-property caption | equals "winLossRatio : EBigDecimal" | verify-true
    get-item "winLossRatio : EBigDecimal" | get-property checked | equals false | verify-true
    get-item "gender : Gender" | get-property caption | equals "gender : Gender" | verify-true
    get-item "gender : Gender" | get-property checked | equals false | verify-true
}
get-window -class WizardDialog | get-button Finish | click
get-view "Model Explorer" | get-tree | get-item "Project/View/TableControl" 
    | get-property "getItemCount()" | equals 0 | verify-true
get-view "Model Explorer" | get-tree | select "Project/View/TableControl" | double-click

with [get-editor TableControl | get-table] {
    get-item name | get-property caption | equals name | verify-true
    get-item height | get-property caption | equals height | verify-true
    get-item eMails | get-property caption | equals eMails | verify-true
    get-property itemCount | equals 3 | verify-true
}

get-view "Model Explorer" | get-tree | select "Project/View/TableControl" | get-menu "Generate Columns" | click
get-window -class WizardDialog | get-editbox -after [get-label Search] | set-text na
get-window -class WizardDialog | get-table | get-property itemCount | equals 3 | verify-true
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | check
    get-item "isProfessional : EBoolean" | check
}
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | get-property checked | equals true | verify-true
    get-item "isProfessional : EBoolean" | get-property caption | equals "isProfessional : EBoolean" | verify-true
    get-item "isProfessional : EBoolean" | get-property checked | equals true | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property caption 
        | equals "playedTournamentTypes : TournamentType" | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property checked | equals false | verify-true
}
get-window -class WizardDialog | get-editbox -after [get-label Search] | set-text ""
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | get-property caption | equals "name : EString" | verify-true
    get-item "name : EString" | get-property selected | equals false | verify-true
    get-item "dateOfBirth : EDate" | get-property caption | equals "dateOfBirth : EDate" | verify-true
    get-item "dateOfBirth : EDate" | get-property checked | equals false | verify-true
    get-item "height : EDouble" | get-property caption | equals "height : EDouble" | verify-true
    get-item "height : EDouble" | get-property checked | equals false | verify-true
    get-item "isProfessional : EBoolean" | get-property caption | equals "isProfessional : EBoolean" | verify-true
    get-item "isProfessional : EBoolean" | get-property checked | equals true | verify-true
    get-item "eMails : EString" | get-property caption | equals "eMails : EString" | verify-true
    get-item "eMails : EString" | get-property checked | equals false | verify-true
    get-item "numberOfVictories : EInt" | get-property caption | equals "numberOfVictories : EInt" | verify-true
    get-item "numberOfVictories : EInt" | get-property checked | equals false | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property caption 
        | equals "playedTournamentTypes : TournamentType" | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property checked | equals false | verify-true
    get-item "winLossRatio : EBigDecimal" | get-property caption | equals "winLossRatio : EBigDecimal" | verify-true
    get-item "winLossRatio : EBigDecimal" | get-property checked | equals false | verify-true
    get-item "gender : Gender" | get-property caption | equals "gender : Gender" | verify-true
    get-item "gender : Gender" | get-property checked | equals false | verify-true
}
with [get-window -class WizardDialog] {
    get-editbox -after [get-label Search] | set-text ""
    get-button "Select All" | click
}
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | get-property checked | equals true | verify-true
    get-item "dateOfBirth : EDate" | get-property checked | equals true | verify-true
    get-item "height : EDouble" | get-property checked | equals true | verify-true
    get-item "isProfessional : EBoolean" | get-property checked | equals true | verify-true
    get-item "eMails : EString" | get-property checked | equals true | verify-true
    get-item "numberOfVictories : EInt" | get-property checked | equals true | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property checked | equals true | verify-true
    get-item "winLossRatio : EBigDecimal" | get-property checked | equals true | verify-true
    get-item "gender : Gender" | get-property checked | equals true | verify-true
}
get-window -class WizardDialog | get-button "Deselect All" | click
with [get-window -class WizardDialog | get-table] {
    get-item "name : EString" | get-property checked | equals false | verify-true
    get-item "dateOfBirth : EDate" | get-property checked | equals false | verify-true
    get-item "height : EDouble" | get-property checked | equals false | verify-true
    get-item "isProfessional : EBoolean" | get-property checked | equals false | verify-true
    get-item "eMails : EString" | get-property checked | equals false | verify-true
    get-item "numberOfVictories : EInt" | get-property checked | equals false | verify-true
    get-item "playedTournamentTypes : TournamentType" | get-property checked | equals false | verify-true
    get-item "winLossRatio : EBigDecimal" | get-property checked | equals false | verify-true
    get-item "gender : Gender" | get-property checked | equals false | verify-true
}
get-window -class WizardDialog | get-button Cancel | click
} -finally {  
	get-view "Model Explorer" | get-tree | select "Project/View" | get-menu "Unset Root EClass" | click
}
