		get-button "Save Project" | click
		} -catch {
			wait 1000
			try {
			get-tree | select "Project/View"
			get-button "Save Project" | click
			} -catch {
				wait 1000
				get-tree | select Project
				get-button "Save Project" | click
			}
		}
	}
	*/
	get-view "Model Explorer" | get-tree | select "Project/View" | get-menu "Unset Root EClass" | click
}
