try {
get-editor View | get-button "Link Root EClass" | click
get-window "Select an EClass." | get-tree | select "bowling/Player \\[org.eclipse.emf.emfstore.bowling.Player\\]" 
    | double-click
get-view "Model Explorer" | get-tree | select "Project/View" | get-menu Control | click
get-editor Control | get-editbox -after [get-label "Name*"] | set-text NoOfVictories
get-editor NoOfVictories | get-button -after [get-label Readonly] | check
get-view "Model Explorer" | get-tree | select "Project/View/NoOfVictories" 
    | get-menu "Feature Path Domain Model Reference" | click
get-editor "Feature Path Domain Model Reference" | get-button "Link Domain Model EFeature" | click
get-window "Select a EStructuralFeature" | get-tree | select "numberOfVictories : EInt" | double-click
with [get-view "Model Explorer"] {
	try {
    get-tree | select Project
    get-button "Save Project" | click
	} -catch {
		wait 1000
		try {
		get-tree | select "Project/View"
		get-button "Save Project" | click
		} -catch {
			wait 1000
			get-tree | select Project
			get-button "Save Project" | click
		}
	}
}
get-editor "Player John" | close
get-editor View | click
get-view "Model Explorer" | get-tree | select "TestProject/Player John" | double-click
get-editor "Player John" | get-editbox -after [get-label "Number Of Victories"] | click-text 1
get-editor "Player John" | get-editbox -after [get-label "Number Of Victories"] | get-property readOnly | equals true 
    | verify-true
get-view "Model Explorer" | get-tree | select "Project/View"

