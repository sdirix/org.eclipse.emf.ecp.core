with [get-view "Model Explorer" | get-tree] {
    select "TestProject \\[trunk, Version 0\\]/Tournament Pro/Matchup" | get-menu Game | click
    select "TestProject \\[trunk, Version 0\\]" | get-menu "Create Branch" | click
}
with [get-window "Create Branch"] {
    get-editbox -after [get-label "New Branch:"] | set-text B
    get-button OK | click
}
get-window Commit | get-button OK | click
