with [get-window "Create new project"] {
    get-editbox -after [get-label "Name:"] |set-text TestProjecttrunk
    get-button OK | click
}
with [get-window "Checkout Branch"] {
    get-table | select "Branch:  trunk  \\[Version: 0\\]"
    get-button OK | click
}
