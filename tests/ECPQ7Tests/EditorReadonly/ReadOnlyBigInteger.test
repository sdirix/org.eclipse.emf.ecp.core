get-editor View | get-editbox -after [get-label "Name*"] | set-text MatchupView
get-view "Model Explorer" | get-tree | select "Project/MatchupView" | get-menu Control | click
get-editor Control | get-editbox -after [get-label "Name*"] | set-text NrOfSpectators
get-view "Model Explorer" | get-tree | select "Project/MatchupView"
get-editor MatchupView | get-button "Link Root EClas" | click
get-window "Select an EClass." | get-tree | select "bowling/Matchup \\[org.eclipse.emf.emfstore.bowling.Matchup\\]" 
    | double-click
get-view "Model Explorer" | get-tree | select "Project/MatchupView/NrOfSpectators" 
    | get-menu "Feature Path Domain Model Reference" | click
get-editor "Feature Path Domain Model Reference" | get-button "Link Domain Model EFeature" | click
get-window "Select a EStructuralFeature" | get-tree | select "nrSpectators : EBigInteger" | double-click

get-view "Model Explorer" | get-tree | select "TestProject/Tournament Amateur/Matchup" | double-click
get-editor Matchup | get-editbox -after [get-label "Nr Spectators"] | set-text ""
get-view "Model Explorer" | get-tree | select "Project/MatchupView/NrOfSpectators" | double-click
get-editor NrOfSpectators | get-button -after [get-label Readonly] | check
with [get-view "Model Explorer"] {
    get-tree | select Project
    try { try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }} -catch{try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }}
}
get-editor Matchup | close
get-view "Model Explorer" | get-tree | select "TestProject/Tournament Amateur/Matchup" | double-click
get-editor Matchup | get-editbox -after [get-label "Nr Spectators"] | click-text
get-editor Matchup | get-editbox -after [get-label "Nr Spectators"] | get-property readOnly | equals true | verify-true

get-view "Model Explorer" | get-tree | select "Project/MatchupView"
get-editor MatchupView | get-button "Delete Reference" | click
with [get-view "Model Explorer"] {
    get-tree | select Project
    try { get-button "Save Project" | click} - catch { wait 500 | get-button "Save Project" | click }
}
