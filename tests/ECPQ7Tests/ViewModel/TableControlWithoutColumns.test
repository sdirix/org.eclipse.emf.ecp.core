Entry-Name: .content

get-editor View | get-button "Link Root EClas" | click
with [get-window "Select an EClass."] {
    get-tree | select "bowling/Tournament \\[org.eclipse.emf.emfstore.bowling.Tournament\\]"
    get-button OK | click
}
with [get-view "Model Explorer" | get-tree] {
    select "Project/View" | get-menu TableControl | click
    select "Project/View/TableControl" | get-menu "Table Domain Model Reference" | click
}
get-editor "Table Domain Model Reference" | get-button "Link Domain Model EFeature" | click
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "matchups : Matchup"
    get-button OK | click
}
with [get-view "Model Explorer" | get-tree] {
    select "Project/View" | get-menu TableControl | click
    select [get-item "Project/View" | get-item TableControl -index 1] | get-menu "Table Domain Model Reference" | click
}
get-editor "Table Domain Model Reference" -index 1 | get-button "Link Domain Model EFeature" | click
with [get-window "Select a EStructuralFeature"] {
    get-tree | select "players : Player"
    get-button OK | click
}
with [get-view "Model Explorer"] {
    get-tree | select "Project/View"
    try { get-button "Save Project" | click } -catch{wait 500 | try { get-button "Save Project" | click } -catch{ wait 500 | get-button "Save Project" | click }}
    get-tree | select "Project/View" | get-menu "Create new project" | click
}
with [get-window "Create Project"] {
    get-editbox -after [get-label "Project Name:"] | set-text test
    get-button Finish | click
}
get-view "Model Explorer" | get-tree | select test | get-menu "New Model Element" | click
with [get-window "Add new model element"] {
    get-tree | select "bowling/Tournament"
    get-button Finish | click
}
with [get-editor "Tournament Pro"] {
    with [get-table] {
        get-property "columns[0].caption" | equals "Validation Status" | verify-true
        get-property "columns[1].caption" | equals "Nr Spectators" | verify-true
        get-property "columns[1].tooltip" | equals "The Nr Spectators of the Matchup" | verify-true
    }
    with [get-table -after [get-label Players]] {
        get-property "columns[0].caption" | equals "Validation Status" | verify-true
        get-property "columns[1].caption" | equals Name | verify-true
        get-property "columns[1].tooltip" | equals "The Name of the Player" | verify-true
        get-property "columns[2].caption" | equals "Date Of Birth" | verify-true
        get-property "columns[2].tooltip" | equals "The Date Of Birth of the Player" | verify-true
        get-property "columns[3].caption" | equals Height | verify-true
        get-property "columns[3].tooltip" | equals "The Height of the Player" | verify-true
        get-property "columns[4].caption" | equals "Is Professional" | verify-true
        get-property "columns[4].tooltip" | equals "The Is Professional of the Player" | verify-true
        get-property "columns[5].caption" | equals EMails | verify-true
        get-property "columns[5].tooltip" | equals "The EMails of the Player" | verify-true
        get-property "columns[6].caption" | equals "Number Of Victories" | verify-true
        get-property "columns[6].tooltip" | equals "The Number Of Victories of the Player" | verify-true
        get-property "columns[7].caption" | equals "Played Tournament Types" | verify-true
        get-property "columns[7].tooltip" | equals "The Played Tournament Types of the Player" | verify-true
        get-property "columns[8].caption" | equals "Win Loss Ratio" | verify-true
        get-property "columns[8].tooltip" | equals "The Win Loss Ratio of the Player" | verify-true
        get-property "columns[9].caption" | equals Gender | verify-true
        get-property "columns[9].tooltip" | equals "The Gender of the Player" | verify-true
    }
}
